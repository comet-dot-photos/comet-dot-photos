{
    "name": "cometserver",
    "version": "3.0.1",
    "scripts": {
        "deployment": "cross-env REDIRECT=TRUE PORT=443 HTTP_PORT=80 CERTFILE=/etc/letsencrypt/live/comet.photos/fullchain.pem KEYFILE=/etc/letsencrypt/live/comet.photos/privkey.pem node cometserver.js",
        "preprocess": "cross-env PORT=8082 PREPROCESSING=TRUE DATASETSFILE=../data/preprocessing/preprocess.json node cometserver.js",
        "server": "cross-env PORT=8082 CERTFILE=/etc/letsencrypt/live/comet.photos/fullchain.pem KEYFILE=/etc/letsencrypt/live/comet.photos/privkey.pem node cometserver.js",
        "local": "cross-env LAUNCHBROWSER=TRUE PORT=8082 node cometserver.js",
        "test": "cross-env TEST=regression1 LAUNCHBROWSER=TRUE PORT=8082 node cometserver.js"
    },
    "engines": {
        "node": ">=22.12"
    },
    "dependencies": {
        "compression": "^1.7.4",
        "cross-env": "^7.0.3",
        "express": "^4.21.1",
        "httpolyglot": "^0.1.2",
        "koffi": "^2.9.1",
        "open": "^10.1.0",
        "socket.io": "^4.8.0"
    },
    "license": "MIT",
    "author": "D. Kurlander"
}
